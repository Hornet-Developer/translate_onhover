{
  "name": "TransOver",
  "version": "0.9",
  "icons": { "48": "to_icon.png", "128": "to_icon.png" },
  "description": "Translate word by mouse pointing",
  "background_page": "background.html",
  "options_page": "options.html",
  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": [
      "lib/jquery-1.5.1.min.js",
      "lib/xregexp-min.js",
      "lib/xregexp-unicode-base.js",
      "lib/tooltip.js",
      "contentscript.js"
    ],
    "css": ["styles.css"],
    "all_frames": true
  }],
  "permissions": [ "http://www.google.com/jsapi", "http://www.google.com/translate_a/t" ]
}

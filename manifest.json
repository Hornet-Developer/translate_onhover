{
  "name": "TransOver",
  "version": "0.4",
  "icons": { "48": "to_icon.png", "128": "to_icon.png" },
  "description": "Translates word on mouse hover",
  "background_page": "background.html",
  "options_page": "options.html",
  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": [
      "lib/jquery-1.5.1.min.js",
      "lib/xregexp-min.js",
      "lib/xregexp-unicode-base.js",
      "lib/tooltip.js",
      "contentscript.js"
    ],
    "all_frames": true
  }],
  "permissions": [ "http://*.google.com/" ]
}
